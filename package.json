{
	"name": "modai",
	"version": "1.0.0",
	"description": "This is an LLM plugin for Obsidian (https://obsidian.md)",
	"main": "main.js",
	"type": "module",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json",
		"lint": "eslint .",
		"format": "prettier --check \"**/*.{ts,mts,css,json,md}\"",
		"format:fix": "prettier --write \"**/*.{ts,mts,css,json,md}\"",
		"knip": "knip",
		"check-all": "npm run lint && npm run format && npm run knip"
	},
	"keywords": [],
	"license": "0-BSD",
	"devDependencies": {
		"@eslint/js": "9.39.2",
		"@types/node": "^25.0.6",
		"esbuild": "0.27.2",
		"eslint-plugin-obsidianmd": "0.1.9",
		"globals": "17.0.0",
		"jiti": "^2.6.1",
		"prettier": "3.7.4",
		"tslib": "2.8.1",
		"typescript": "^5.9.3",
		"typescript-eslint": "8.52.0"
	},
	"dependencies": {
		"diff": "^8.0.2",
		"obsidian": "latest"
	}
}
